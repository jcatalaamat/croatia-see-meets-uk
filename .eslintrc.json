{
    "extends": ["react-app", "airbnb", "plugin:jsx-a11y/recommended"],
    "plugins": ["jsx-a11y"],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "prefer-destructuring": ["error", {
            "array": false,
            "object": true
          }, {
            "enforceForRenamedProperties": false
          }]
    }
  }